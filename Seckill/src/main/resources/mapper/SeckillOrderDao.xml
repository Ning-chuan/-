<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yuziyan.seckill.dao.SeckillOrderDao">

    <insert id="addSeckillOrder" parameterType="seckillOrder">
        insert into seckill_order (seckill_item_id,user_id,state,create_time)
        values (#{seckillItemId},#{userId},#{state},#{createTime})
    </insert>

</mapper>